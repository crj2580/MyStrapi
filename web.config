<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="strapi">
                    <match url="^api/(.*)" />
                    <action type="Rewrite" url="http://localhost:1337/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
